@startuml player-with-axes
!include lib/game.puml!core
!include lib/i18n.puml!en
skinparam ranksep 20
skinparam nodesep 1

$iconify_players()

$fill_axes()
$join_axes_3x3(hidden, hidden)
@enduml

@startuml matrix-with-axes
!include lib/game.puml!core
!include lib/i18n.puml!en
skinparam ranksep 40
skinparam nodesep 1

<style>
  label {
    FontSize: 30;
  }
</style>

$iconify_players()

$fill_axes()
$join_axes_3x3(hidden, hidden)

$fill_matrix()
$join_matrix_3x3()
@enduml

@startuml matrix-with-protocols
!include lib/game.puml!core
!include lib/i18n.puml!en
skinparam ranksep 40
skinparam nodesep 60

<style>
  .matrix {
    FontSize: 30;
    FontColor: Grey;
  }
</style>

$iconify_grid_3x1()
$textify_grid_1x3()

$empty_axes()
$join_axes_3x3()

$fill_matrix()
$join_matrix_3x3()
@enduml
